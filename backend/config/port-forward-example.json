{
  "services": [
    {
      "name": "tcp-forward-8080",
      "addr": ":8080",
      "handler": {
        "type": "tcp",
        "metadata": {
          "forward": "192.168.1.1:80"
        }
      }
    },
    {
      "name": "udp-forward-53",
      "addr": ":10053",
      "handler": {
        "type": "udp",
        "metadata": {
          "forward": "192.168.1.1:53"
        }
      }
    },
    {
      "name": "http-proxy",
      "addr": ":8000",
      "handler": {
        "type": "http"
      }
    }
  ],
  "chains": []
} 