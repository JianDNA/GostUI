        "auther": "auther-0",
        "observer": "observer-0",
        "limiter": "limiter-0",
        "metadata": {
          "observer.period": "5s",
          "observer.resetTraffic": false
        }
      },
      "listener": {
        "type": "tcp"
      },
      "metadata": {
        "enableStats": true,
        "observer.period": "5s",
        "observer.resetTraffic": false,
        "observer.batch": true,
        "userId": 1,
        "username": "admin",
        "ruleId": 2,
        "ruleName": "node转发",
        "description": ""
      }
    },
    {
