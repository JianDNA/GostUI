{
  "services": [
    {
      "name": "forward-tcp-9080",
      "addr": ":9080",
      "observer": "observer-0",
      "handler": {
        "type": "tcp",
        "chain": "chain-tcp-9080",
        "metadata": {
          "observer.period": "30s",
          "observer.resetTraffic": true
        }
      },
      "listener": {
        "type": "tcp"
      },
      "metadata": {
        "enableStats": true,
        "observer.period": "30s",
        "observer.resetTraffic": true,
        "userId": 1,
        "username": "admin",
        "ruleId": 7,
        "ruleName": "Admin HTTP Proxy",
        "description": "管理员HTTP代理规则"
      }
    },
    {
      "name": "forward-tcp-29082",
      "addr": ":29082",
      "observer": "observer-0",
      "handler": {
        "type": "tcp",
        "chain": "chain-tcp-29082",
        "metadata": {
          "observer.period": "30s",
          "observer.resetTraffic": true
        }
      },
      "listener": {
        "type": "tcp"
      },
      "metadata": {
        "enableStats": true,
        "observer.period": "30s",
        "observer.resetTraffic": true,
        "userId": 1,
        "username": "admin",
        "ruleId": 5,
        "ruleName": "Admin Rule 23",
        "description": ""
      }
    }
  ],
  "chains": [
    {
      "name": "chain-tcp-9080",
      "hops": [
        {
          "name": "hop-0",
          "nodes": [
            {
              "addr": "127.0.0.1:8081",
              "connector": {
                "type": "tcp"
              }
            }
          ]
        }
      ]
    },
    {
      "name": "chain-tcp-29082",
      "hops": [
        {
          "name": "hop-1",
          "nodes": [
            {
              "addr": "127.0.0.1:3000",
              "connector": {
                "type": "tcp"
              }
            }
          ]
        }
      ]
    }
  ],
  "observers": [
    {
      "name": "observer-0",
      "plugin": {
        "type": "http",
        "addr": "http://localhost:3000/api/gost-plugin/observer",
        "timeout": "10s"
      }
    }
  ],
  "api": {
    "addr": ":18080",
    "pathPrefix": "/api",
    "accesslog": false
  }
}