{
  "services": [
    {
      "name": "forward-tcp-2999",
      "addr": ":2999",
      "observer": "observer-0",
      "handler": {
        "type": "tcp",
        "chain": "chain-tcp-2999",
        "auther": "auther-0",
        "observer": "observer-0",
        "limiter": "limiter-0",
        "metadata": {
          "observer.period": "5s",
          "observer.resetTraffic": true
        }
      },
      "listener": {
        "type": "tcp"
      },
      "metadata": {
        "enableStats": true,
        "observer.period": "5s",
        "observer.resetTraffic": true,
        "userId": 2,
        "username": "test",
        "ruleId": 3,
        "ruleName": "Test HTTPS Proxy",
        "description": "测试用户HTTPS代理规则"
      }
    },
    {
      "name": "forward-tcp-6443",
      "addr": ":6443",
      "observer": "observer-0",
      "handler": {
        "type": "tcp",
        "chain": "chain-tcp-6443",
        "auther": "auther-0",
        "observer": "observer-0",
        "limiter": "limiter-0",
        "metadata": {
          "observer.period": "5s",
          "observer.resetTraffic": true
        }
      },
      "listener": {
        "type": "tcp"
      },
      "metadata": {
        "enableStats": true,
        "observer.period": "5s",
        "observer.resetTraffic": true,
        "userId": 1,
        "username": "admin",
        "ruleId": 1,
        "ruleName": "Admin HTTPS Proxy",
        "description": "管理员HTTPS代理规则"
      }
    },
    {
      "name": "forward-tcp-8080",
      "addr": ":8080",
      "observer": "observer-0",
      "handler": {
        "type": "tcp",
        "chain": "chain-tcp-8080",
        "auther": "auther-0",
        "observer": "observer-0",
        "limiter": "limiter-0",
        "metadata": {
          "observer.period": "5s",
          "observer.resetTraffic": true
        }
      },
      "listener": {
        "type": "tcp"
      },
      "metadata": {
        "enableStats": true,
        "observer.period": "5s",
        "observer.resetTraffic": true,
        "userId": 1,
        "username": "admin",
        "ruleId": 2,
        "ruleName": "Admin HTTP Proxy",
        "description": "管理员HTTP代理规则"
      }
    }
  ],
  "chains": [
    {
      "name": "chain-tcp-2999",
      "hops": [
        {
          "name": "hop-0",
          "nodes": [
            {
              "addr": "1.1.1.1:443",
              "connector": {
                "type": "tcp"
              }
            }
          ]
        }
      ]
    },
    {
      "name": "chain-tcp-6443",
      "hops": [
        {
          "name": "hop-1",
          "nodes": [
            {
              "addr": "1.1.1.1:443",
              "connector": {
                "type": "tcp"
              }
            }
          ]
        }
      ]
    },
    {
      "name": "chain-tcp-8080",
      "hops": [
        {
          "name": "hop-2",
          "nodes": [
            {
              "addr": "1.1.1.1:80",
              "connector": {
                "type": "tcp"
              }
            }
          ]
        }
      ]
    }
  ],
  "authers": [
    {
      "name": "auther-0",
      "plugin": {
        "type": "http",
        "addr": "http://localhost:3000/api/gost-plugin/auth",
        "timeout": "5s"
      }
    }
  ],
  "observers": [
    {
      "name": "observer-0",
      "plugin": {
        "type": "http",
        "addr": "http://localhost:3000/api/gost-plugin/observer",
        "timeout": "10s"
      }
    }
  ],
  "limiters": [
    {
      "name": "limiter-0",
      "plugin": {
        "type": "http",
        "addr": "http://localhost:3000/api/gost-plugin/limiter",
        "timeout": "5s"
      }
    }
  ]
}