        "metadata": {
          "observer.period": "120s",
          "observer.resetTraffic": true
        }
      },
      "listener": {
        "type": "tcp"
      },
      "metadata": {
        "enableStats": true,
        "observer.period": "120s",
        "observer.resetTraffic": true,
        "userId": 1,
        "username": "admin",
        "ruleId": 5,
        "ruleName": "Admin Rule 23",
        "description": "",
        "listenAddress": "127.0.0.1",
        "listenAddressType": "ipv4"
      }
    },
    {
      "name": "forward-tcp-9080",
      "addr": "127.0.0.1:9080",
      "observer": "observer-0",
      "handler": {
        "type": "tcp",
        "chain": "chain-tcp-9080",
        "metadata": {
          "observer.period": "120s",
          "observer.resetTraffic": true
        }
      },
      "listener": {
        "type": "tcp"
      },
      "metadata": {
        "enableStats": true,
        "observer.period": "120s",
